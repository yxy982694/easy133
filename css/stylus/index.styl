// 重置窗口部分样式
.window{
	border: 1px solid transparent;
	background: #fff;
	padding: 10px;
	-moz-border-radius: 0 !important;
	-webkit-border-radius: 0 !important;
	border-radius: 0 !important
	filter: none !important;// 去掉针对ie8的背景色
}
.window-header{
	border: 0 !important;
	border-bottom: 1px $table_td_color_border !important;
	padding: 0 !important;
	margin: 0 !important;
}
.window-header .panel-title{
	text-align: center;
}
.messager-window{
	border: 0 !important;
}
.messager-window .panel-title{
	text-align: left;
	background: none !important;
}
.window .window-body{
	border: 0 !important;
}
.panel-header{
	filter: none !important;
}
.win-center-container .panel-body{
	padding: 30px 20px 0 20px !important;
}
/* 第一个center */
.win-center-container{
	border: 0 !important;
}
/* 第一个center中的north */
.layout-panel-north .panel-body{
	border: 0 !important;
}
.layout-panel-south .panel-body{
	border: 0 !important;
}
/* 第一个center中的center */
.table-center-container{
	border: 0 !important;
}
.window-proxy-mask, .window-mask{
	background: #000;
}
.newbuild-list{
	position: relative;
	margin-top: 10px;
}
.newbuild-list+.newbuild-list{
	margin-top: 20px;
}
.newbuild-list .newbuild-list-label{
	position: relative;
	font-size: $font-size-small;
	width: 35px;
	text-align: right;
	display: inline-block;
}
.newbuild-list0{
	height: 25px;
}
.newbuild-list1{
	height: 55px;
}
.newbuild-list-div{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	padding-left: 40px;
	box-sizing: border-box;
}
.newbuild-list-input{
	width: 100%;
	border: 1px solid $input_border_color !important;
	padding-left: 10px;
	height: 25px;
	color: #666;
}
.newbuild-list-input:focus{
	border: 1px solid transparent !important;
	outline: $select_down_selected_color solid 1px !important;
}
.newbuild-list-area{
	width: 100%;
	border: 1px solid $input_border_color !important;
	padding: 0 10px;
}
.newbuild-list-area:focus{
	border: 1px solid transparent !important;
	outline: $select_down_selected_color solid 1px !important;
}
.newbuild-list-span{
	color: #f00;
	position: absolute;
	top: 4px;
	left: 0;
	font-size: $font-size-small;
	display: inline-block;
}
.import-red{
	color: #f00;
	position: absolute;
	left: 20px;
	bottom: -14px;
	font-size: $font-size-small;
}
// alert弹框样式
.dialog-button{
	border: 0 !important;
}
.dialog-button{
	background: $white !important;
}
.messager-button .l-btn,
.dialog-button .l-btn{
	background: $select_down_selected_color !important;
	border: 0 !important;
	color: #fff !important;
	margin-left: 0;
	width: auto;
	padding: 5px 15px;
	-moz-border-radius: 0 !important;
	-webkit-border-radius: 0 !important;
	border-radius: 0 !important
	height: 16px;
	box-sizing: content-box;
}
.messager-button .l-btn .l-btn-left{
	background-image: none !important;
	padding: 0 !important;
}
.dialog-button .l-btn .l-btn-text{
	margin: 0;
	line-height: normal;
	font-size: $font-size-small !important;
}
.messager-body{
	padding-top: 20px !important;
}
.messager-window .panel-title{
	font-size: $font-size-large;
	color: $home_font_color;
}
.messager-window .messager-body{
	color: $input_font_color;
}
// 按钮
.newbuild-btns{
	margin-top: 20px;
}
// 自定义确定和取消按钮样式
.eoms-btn{
	font-size: $font-size-small !important;
	display: inline-block;
	padding: 5px 15px;
	background-color: $select_down_selected_color;
	border: 1px solid $select_down_selected_color;
	color: #fff;
	cursor: pointer;
	float: right;
	margin-left: 10px;
}
.eoms-btn:focus{
	outline:none;
    filter: chroma(color=#000000);
}
.eoms-cancel:focus{
	outline:none;
	filter: chroma(color=#000000);
}
.eoms-cancel{
	font-size: $font-size-small !important;
	display: inline-block;
	padding: 5px 15px;
	background-color: $white;
	border: 1px solid $input_border_color;
	color: $input_font_color;
	cursor: pointer;
	float: right;
	margin-left: 10px;
}
// 弹窗确定和取消按钮样式
.messager-button{
	overflow: hidden;
}
.messager-button .l-btn{
	float: right;
	margin-left: 10px;
}
.messager-button .l-btn+.l-btn{
	background-color: $white !important;
	border: 1px solid $input_border_color !important;
	color: $input_font_color !important;
}
.messager-icon{
	display: none;
}
// 按钮定义结束
.message-fulcontainer{
	height: 203px;
	margin-top: 10px;
}
.message-container{
	margin: 0 22px;
	.message-item{
		position: relative;
		overflow: hidden;
		height: 20px;
		&+.message-item{
			margin-top: 18px
		}
		.text-left{
			position: absolute;
			text-align: left;
			padding-left: 15px;
			padding-right: 100px;
			cursor: pointer;
			height: 20px;
			no-wrap();
			width: 100%;
			box-sizing: border-box;
			.text-name{
				display: inline-block;
				width: 90px;
				font-size: $font-size-medium;
				color: $color-textblue;
			} 
			.message-quot{
				color: #bed0f6;
				font-size: 20px;
				font-style: normal;
			} 
			.text-content-title{
				margin: 0 3px;
				font-style: normal;
				font-size: $font-size-medium;
			}  
		}
		.text-right{
			width: 100px;
			float: right;
			text-align: right;
			font-size: $font-size-small;
			color: $color-textgray;
			margin-top: 4px;
		}  
	}
}
.message-color{
	color: $color-textblue;
	margin-right: 10px;
} 
.message-bg{
	display: inline-block;
	background: url('../images/newMessage_bg.png') no-repeat;
	background-size: 100% 100%;
	width: 14px;
	height: 14px;
}
@media screen and (min-height: 980px){
	.message-fulcontainer{
		height: 360px;
	}
	.message-container .message-item{
		margin-top: 33px;
		&+.message-item{
			margin-top: 45px; 
		}  
	} 
}
      
   