@import "./reset.styl";
@import "./_mixin.styl";
/*  select下拉框样式1250px */
.textbox {
  border: none !important;
  border-radius: 0 !important;
  width: 100% !important;
}
.combo-arrow {
  background-color: #fff !important;
}
.combo-p {
  background-color: #fff;
}
.left-item-label {
  margin-right: 8px;
}
.textbox-text {
  color: #606266;
  font-size: $font-size-small !important; 
}
.combobox-item {
  color: #606266;
  font-size: $font-size-small !important; 
}
.combo-panel {
  border: 1px solid #f5f7fa !important;
}
.combobox-item-selected {
  background-color: #e4e7ed !important;
  color: #409eff !important;
  font-weight: bold;
}
.panel-fit,
.panel-fit body{
	overflow: auto !important;
	overflow-y: hidden !important;
	overflow-x: auto;
}
#Loading{
	position: absolute;
	z-index: 100;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	background: $loadimg_bg;
	text-align: center;
	padding-top: 20%;
	color: $header_left_bg;
}
.panel-body{
	padding: 0px !important;
	background-color: transparent !important;
}
.tabs-panels .panel+.panel .panel-body{
	padding-top: 8px !important;
}
.tabs-panels .panel+.panel{
	background-color: $home_container_bg !important;
}
/* 导航菜单样式222 */
.header-left{
	width: 36%;
	float: left;
	padding-left: 20px;
	background-color: $header_left_bg;
	box-sizing: border-box;
	height: 100%;
	position: relative;
}
.header-left h1{
	float: left;
	margin-top: 15px;
	margin-right: 10px;
}
.header-left h3{
	color: $white;
	font-size: $font-size-large-x;
	width: 100%;
	text-align: center;
	position: absolute;
	left: 0;
	top: 25px;
	padding-left: 110px;
	box-sizing: border-box;
}
.header-center{
	position: absolute;
	right: -45px;
	top: 0;
	height: 70px;
	width: 90px;
	z-index: 99;
	overflow: hidden;
	background-color: transparent;
	font-size: 0;
}
.header-center .skew-img{
	height: 100%;
	width: 100%;
}
.header-right{
	width: 64%;
	float: left;
	position: relative;
	padding: 11px 20px 0 33px;
	background-color: $header_right_bg;
	box-sizing: border-box;
	height: 100%;
	z-index: 55;
}
.header-business{
	width: 100%;
	box-sizing: border-box;
	font-size: $font-size-medium;
	padding-bottom: 5px;
	padding-right: 10px;
	overflow: hidden;
	height: 21px;
}
.header-business-item{
	background: transparent;
	float: right;
}  
.header-business-item .icon-xiaoxi{
	position: relative;
}
.header-business-item:last-child{
	padding: 0 20px 0 0;
	max-width: 180px;
	cursor: auto !important;
}
.header-business-item+.header-business-item{
	cursor: pointer;
	border-right: 1px solid $header_business_border_color;
	color: $header_business_font_color;
	padding: 0 20px;
}
.header-business .header-business-item:first-child{
	padding: 0 10px 0 18px;
	color: $header_business_font_color;
	cursor: pointer;
}
.user-name{
	color: $header_user-name_color !important;
	height: 18px;
	line-height: 18px;
}
.navbar{
	height: 30px;
	width: 100%;
	margin-top: 2px;
}
#pp{
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.navbar .l-btn{
	color: $white;
	flex: 1;
}
.navbar .l-btn:hover
.navbar .m-btn-plain-active{
	background-color: $header_menu_hover_bg;
	border: 1px solid transparent !important;
	border-radius: 0;
	color: $white !important;
}
.fa-sort-desc{
	position: absolute;
	right: 7px;
	top: 4px;
}
.navbar .panel-body{
	width: 100% !important;
}
.m-btn-downarrow{
	margin-top: -7px;
}
// 为顶部导航菜单添加蓝色背景色
// .menu-item
.menu{
	border: 0 !important;
	padding: 0 !important;
}
#mm{
	background-color: $white;
	border: 1px solid $home_module_border_color !important;
	width: 120px !important;
}
.menu-item{
	background-color: $header_menu_hover_bg;
	border: 0;
}
#mm .menu-item{
	background-color: $white;
}
.menu-item .menu-text{
	padding-left: 10px;
	color: $white;
}
#mm .menu-text{
	padding-left: 10px;
	color: #666;
}
.menu-item.menu-active{
	border-color:none !important;
	color:$white !important;
	background: $header_left_bg !important;
	border-radius: 0 !important;
}
#mm .menu-active{
	background: $home_module_border_color !important;
}
.m-btn-downarrow,
.menu-rightarrow{
	color: #fff !important;
	background: none !important;
	font-size: 14px !important;
}
.menu-line{
	display: none;
}
	// 右键菜单样式
#mm .cs-tab-menu{
	width:120px !important;
}
	// tab切换样式
.tabs-header,
.tabs-inner,
.tabs{
	border: none !important;
	background-color: $white !important;
	background: $white !important;
	filter: auto !important;
}
.tabs-header{
	padding-top: 0 !important;
}
// .tabs-disabled{
// 	opacity: 1 !important;
// }
.easyui-tabs .tabs-header{
	height: 37px !important;
}
.easyui-tabs  .tabs-wrap{
	height: 100% !important;
}
.easyui-tabs .tabs{
	height: 100% !important;
}
.easyui-tabs .tabs-inner{
	height: 37px !important;
	line-height: 37px !important;
}
.easyui-tabs li.tabs-selected .tabs-inner{
	border-bottom: 2px solid $home_tab_border_color !important;
	font-weight: normal !important;
}
	// 中间内容部分样式
.home-container{
	background-color: $home_container_bg;
}
.home-container-three{
	margin: 0 8px;
	padding: 8px 0;
	overflow: hidden;
}
// 左边样式
.home-left{
	float: left;
	width: 20%;
	box-sizing: border-box;
}
	// 中间样式
.home-center{
	float: left;
	width: 60%;
	padding: 0 8px;
	box-sizing: border-box;
}
.tabs-panels>.panel-htop+.panel-htop .panel-body{
	overflow hidden !important;
}
.home-center-container{
	box-sizing: border-box;
}
	// 右边样式
.home-right{
	float: left;
	width: 20%;
	box-sizing: border-box;
}
	// 底部样式
.home-footer{
	padding: 8px 0;
	text-align: center;
	font-size: $font-size-small;
	background-color: $footer_bg;
	color: $footer_font_color;
}
.info-text{
	margin-left: 40px;
}
// 嵌套iframe样式
iframe{
	height: 100% !important;
	position: static !important;
	width: 100% !important;
}
// 首页中间内容公共样式2
.home-business-item{
	border: 1px solid $home_module_border_color;
	box-sizing: border-box;
	background-color: $white;
}
.section-title{
	padding: 15px 18px;
	overflow: hidden;
}
@media screen and (min-height:980px) {
	.section-title{
		padding: 15px 30px
	}
}
.section-title-name{
	font-size: $font-size-medium;
	color: $color-textblack;
	font-weight: bold;
	float: left;
}     
.section-title-click{ // 可点击的编辑或全部按钮
	cursor: pointer;
	font-size: $font-size-small;
	color: $color-textgray;
	float: right;
	margin-top: 1px;
}
// 我的待办
.mytodo-container{
	height: 370px;
}
.hidden-dayWork{
	display: none;
}
.show-dayWork{
	display: block;
}
// 下拉框样式
.left-item{
	position: relative;
	height: 25px;
	margin-top: 10px;
}
.left-item-label{
	display: block;
	padding-top: 4px;
}
.left-item-div{
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	height: 100%;
	padding-left: 72px;
	box-sizing: border-box;
}
.todo-header{
	height: 194px;
	width: 100%;
	background: rgb(131,198,245);
	position: relative;
	box-sizing: border-box;
}
.todo-header-bg{
	position: absolute;
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
}
.todo-header-user{
	width: 100%;
	position: absolute;
	top: 10px;
	text-align: center;
}
.toto-username{
	font-size: $font-size-medium;
	font-weight: bold;
	line-height: 20px;
	color: #fff;
	no-wrap();
	margin-left: 10px;
	display: inline-block;
	vertical-align: middle;
}
.todo-userimg{
	height: 60px;
	width: 60px;
	border: 2px solid #fff;
	border-radius: 50%;
	position: relative;
	display: inline-block;
	vertical-align: middle;
}
.userimg-input{
	position: absolute;
	left: 0px;
	top: 0;
	height: 100%;
	width: 100%;
	border-radius: 50%;
	cursor: pointer;
	opacity: 0;
	// filter: alpha(opacity=0);
}
.userimg-img{
	position: absolute;
	left: 0;
	top: 0;
	height: 100%;
	width: 100%;
	border-radius: 50%;
}
.todo-usertext{
	text-align: left;
	color: #fff;
	padding: 0 20px;
	padding-top: 77px;
	.left-item{
		position: relative;
		height: 25px;
		margin-top: 10px;
	}
	.left-item-label{
		display: block;
		padding-top: 4px;
	}
	.left-item-div{
		position: absolute;
		top: 0;
		left: 0;
		right: 0;
		height: 100%;
		padding-left: 72px;
		box-sizing: border-box;
	}
}
.todo-items{
	height: 176px;
	font-size: $font-size-medium;
	overflow: auto;
}  
.accordion .accordion-header{
	border: none !important;
	background: #fff !important;
}
.panel-title{
	color: $home_font_color;
	font-weight: normal;
}
.todo-bottom-title .fa{
	font-size: 16px;
	position: absolute;
	right: 17px;
	top: 7px;
	margin: 0;
	color: $color-textgray;
	font-weight: normal;
}
.todo-bottom-item{
	margin-top: 20px;
}
.todo-bottom-title{
	display: block;
	height: 32px;
	line-height: normal;
	border-bottom: none;
	position: relative;
	text-align: left;
	cursor: pointer;
}
.todo-item-p{
	height: 100%;
	position: relative;
	cursor: pointer;
}
.todo-itemvalue{
	font-weight: bold;
	color: $color-textblue;
	font-size: $font-size-medium;
	position: absolute;
	top: 8px;
	right: 37px;
	cursor: pointer;
}
.need-item{
	width: 190px;
	display: inline-block;
	overflow: hidden;
	height: 100% ;
	margin-left: 20px;
	cursor: pointer;
}   
.todo-itemname{
	position: absolute;
	display: inline-block;
	top: 7px;
	left: 36px;
}
.todo-bg0{
	width: 26px;
	height: 26px;
	margin-right: 10px;
	margin-top: 3px;
} 
.todo-bg1{
   width: 26px;
   height: 26px;
   margin-right: 10px;
   margin-top: 3px;
} 
.todo-bg2{
  width: 27px;
  height: 26px;
  margin-right: 10px;
  margin-top: 3px; 
} 
.todo-sublist{
	height: 35px;
	box-sizing: border-box;
	padding-left: 28px;
	padding-right: 25px;
	cursor: pointer;
	position: relative;
	width: 100%;
}
.todo-listvalue{
	color: #356ce1;
	position: relative;
	float: right;
	top: 50%;
	right: 0;
	transform: translate(0,-50%);
	-ms-transform: translate(0,-50%);
}
.todo-listtext{
	position: absolute;
	top: 50%;
	transform: translate(0,-50%);
	-ms-transform: translate(0,-50%);
	left: 35px;  
}
.todo-sublisteven{
	background-color: #c9e8f0;
}
@media screen and (min-height: 980px){
	.mytodo-container{
		height: 600px;
	}
	.todo-header{
		height: 350px;
	}
	.todo-header-user{
		top: 20px;
	}
	.todo-userimg{
		height: 80px;
		width: 80px;
	}
	.todo-usertext{
		padding-top: 115px;
		.left-item{
			margin-top: 35px;
			&+.left-item{
				margin-top: 40px;
			}
		}
	}
	.todo-items{
		height: 250px;
	}
	.todo-itemvalue{
		right: 57px;
	}
	.need-item{
		margin-left: 30px;
	}  
	.todo-bottom-item{
		margin-top: 36px;
	}
	.todo-bottom-title .fa{
		right: 30px;
	}
} 
