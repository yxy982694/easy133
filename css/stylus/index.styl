// 最新公告
.lastnew-fulcontainer{
	height: 240px;
} 
.lastnew-container{
	position: relative;
}
.swiper-container{
	height: 196px;
	width: 100%;
}
.swiper-wrapper,
.swiper-slide{
	width: 100% !important;
}
.lastnew-container .pagination {
  position: absolute;
  z-index: 20;
  right: 10px;
  top: 50%;
  transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  width: 15px;
  text-align: center;
}
.swiper-pagination-switch {
  display: block;
  width: 10px;
  height: 15px;
  background: rgb(164,209,255);
  cursor: pointer;
}
.swiper-pagination-switch+.swiper-pagination-switch{
	margin-top: 10px;
}
.swiper-active-switch {
  background: $select_down_selected_color;
}
.lastest-item{
	margin: 6px 35px 0 20px;
	font-size: $font-size-small;
	overflow: hidden;
	position: relative;
	&+.lastest-item{
		margin-top: 24px;
	}  
	.text-left{
		color: #F3BF44;
		height: 14px;
		width: 70px;
		float: left;
		.icon-jiantoucu{
			margin-right: 5px;
			font-size: 13px;
			color: $jiantou_color;
		} 
	}
	.text-center{
		height: 14px;
		line-height: 14px;
		text-align:left;
		color: $color-textblack;
		no-wrap();
		cursor: pointer;
		position: absolute;
		padding-left: 80px;
		padding-right: 100px;
		width: 100%;
		box-sizing: border-box;
		left: 0;
		top: 0;
	} 
	.text-right{
		width:100px;
		height: 14px;
		color: $color-textgray;
		text-align: right;
		float: right;
	} 
}
@media screen and (min-height: 980px){
	.lastnew-fulcontainer{
		height: 300px;
	} 
	.swiper-container{
		height: 256px;
		width: 100%;
	}
	.lastest-item{
		margin: 18px 35px 0 20px;
		&+.lastest-item{
			margin-top: 33px; 
		}
	}      
}
// 待办列表
.upcomlist-fulcontainer{
	height: 352px;
	position: relative;
	overflow: hidden;
	margin-top: 10px;
}
.upcomlist-container{
