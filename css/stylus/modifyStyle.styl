@import "./_mixin.styl";
/* 加载动图 */
#Loading{
	position:absolute;
	z-index:103;
	top:0px;
	left:0px;
	width:100%;
	height:100%;
	background:$loadimg_bg;
	text-align:center;
	padding-top: 20%;
	color: $loadimg_color;
	box-sizing: border-box;
}
.fa-spinner{
	font-size: 20px;
	width: 30px;
	height: 30px;
	display: block;
	color: $header_left_bg;
}
// .load-img{
// 	width: 20px;
// }
/* select下拉框样式 */
.textbox {
  border: 1px solid $home_module_border_color !important;
  border-radius: 0 !important;
  width: 100% !important;
}
.combo-arrow {
  background-color: #fff !important;
}
.combo-p {
  background-color: #fff;
}
.select-item{
	position: relative;
	height: 25px;
}
.left-item-label {
  margin-top: 4px;
}
.left-item-div{
	position: absolute;
	top: 0;
	left: 0;
	width: 98%;
	padding-left: 70px;
	box-sizing: border-box;
}
.textbox-text {
  color: #606266;
}
.combobox-item {
  color: #606266;
}
.combo-panel {
  border: 1px solid #f5f7fa !important;
}
.combobox-item-selected {
  background-color: #e4e7ed !important;
  color: #409eff !important;
  font-weight: bold;
}
/* 重置框架样式2px */
.tabs-header{
	padding-top: 0;
}
/* 重置框架样式14px */
.panel-body,
span,
.pagination-info,
.tabs-title{
	font-size: $font-size-small !important;
}
.panel-header, .panel-body{
	border-color: $home_module_border_color !important;
}
.panel-header{
	filter: none !important;// 去掉针对ie8的背景色
}
// 重置窗口部分样式
.window{
	border: 1px solid #a4d1ff;
	background: #fff;
	padding: 10px;
	-moz-border-radius: 0 !important;
	-webkit-border-radius: 0 !important;
	border-radius: 0 !important
	filter: none !important;// 去掉针对ie8的背景色
}
.window-header{
	border: 0 !important;
	border-bottom: 1px $table_td_color_border !important;
	padding: 0 !important;
	margin: 0 !important;
}
.window-header .panel-title{
	text-align: center;
}
.messager-window{
	border: 0 !important;
}
.messager-window .panel-title{
	text-align: left;
}
.window .window-body{
	border: 0 !important;
}
/* 第一个center */
.win-center-container{
	border: 0 !important;
}
/* 第一个center中的north */
.layout-panel-north .panel-body{
	border: 0 !important;
}
.layout-panel-south .panel-body{
	border: 0 !important;
}
/* 第一个center中的center */
.table-center-container{
	border: 0 !important;
}
