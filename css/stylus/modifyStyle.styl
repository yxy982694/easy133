// 按钮样式
.eoms-btn-disabled{
	cursor: not-allowed;
	opacity: 0.5;
	// filter: alpha(opacity=50);
}
.datagrid-cell .eoms-btn{
	padding: 3px 13px !important;
}
button:focus{
	outline:0;
}
.l-btn{
	-moz-border-radius: 0 !important;
	-webkit-border-radius: 0 !important;
	border-radius: 0 !important
}
.l-btn-left{
	-moz-border-radius: 0 !important;
	-webkit-border-radius: 0 !important;
	border-radius: 0 !important
}
.eoms-btns .l-btn.l-btn-disabled{
	cursor: not-allowed !important;
}
.eoms-btn{
	font-size: $font-size-small !important;
	display: inline-block;
	padding: 5px 15px;
	background-color: $select_down_selected_color;
	border: 1px solid $select_down_selected_color;
	color: #fff;
	cursor: pointer;
	// float: right;
	margin-left: 10px;
}
.eoms-btn:focus{
	outline:none;
    filter: chroma(color=#000000);
}
.eoms-cancel:focus{
	outline:none;
	filter: chroma(color=#000000);
}
.eoms-cancel{
	font-size: $font-size-small !important;
	display: inline-block;
	padding: 5px 15px;
	background-color: $white;
	border: 1px solid $input_border_color;
	color: $input_font_color;
	cursor: pointer;
	float: right;
	margin-left: 10px;
}
// 自定义确定和取消按钮样式
.eoms-btn-container{
	padding-top: 5px;
}
.eoms-btns .l-btn{
	font-size: $font-size-small !important;
	display: inline-block !important;
	background: $select_down_selected_color !important;
	border: 0 !important;
	padding: 0 15px;
	height: 26px !important;
	line-height: 26px !important;
	color: #fff !important;
	cursor: pointer !important;
	background-image: none !important;
}
.eoms-btns .l-btn .l-btn-left{
	background-image: none !important;
	padding: 0 !important;
	height: 26px !important;
	line-height: 26px !important;
}
.eoms-btns .l-btn .l-btn-text{
	margin: 0;
	height: 26px !important;
	line-height: 26px !important;
}
//alert中的按钮  
.dialog-button,
.messager-button{
	border: 0 !important;
	overflow: hidden;
	background: $white !important;
}
.messager-button .l-btn,
.dialog-button .l-btn{
	background: $select_down_selected_color !important;
	border: 0 !important;
	color: #fff !important;
	margin-left: 10px;
	width: auto;
	padding: 5px 15px;
	-moz-border-radius: 0 !important;
	-webkit-border-radius: 0 !important;
	border-radius: 0 !important
	height: 16px;
	box-sizing: content-box;
	float: right;
}
.messager-button .l-btn .l-btn-left{
	background-image: none !important;
	padding: 0 !important;
}
.dialog-button .l-btn .l-btn-text{
	margin: 0;
	line-height: normal;
}
.messager-button .l-btn+.l-btn{
	background-color: $white !important;
	border: 1px solid $input_border_color !important;
	color: $input_font_color !important;
}
.messager-icon{
	display: none;
}
// 右键菜单样式
#mm{
	border: 0 !important;
	padding: 0 !important;
	background-color: $white;
	border: 1px solid $home_module_border_color !important;
	width: 120px !important;
}
#mm .menu-item{
	border: 0;
	background-color: $white;
}
#mm .menu-text{
	padding-left: 10px;
	color: #666;
}
#mm .menu-active{
	border-color:none !important;
	color:$white !important;
	border-radius: 0 !important;
	background: $home_module_border_color !important;
}
// 分页样式
.pagination-info{
	letter-spacing: 1px;
}
.pagination-btn-separator{
	display: none;
}
.pagination-span{
	display: inline-block;
	margin-left: 1px;
}
.l-btn-plain-selected{
	background: none !important;
	color: $select_down_selected_color !important;
}
// 文本框
input{
	border: 1px solid $home_module_border_color !important;
}
input:focus{
	outline: none !important;
	box-shadow: none !important;
	border: 1px solid $select_down_selected_color !important;
}